{
  "name": "vue-music",
  "version": "0.1.0-beta-4",
  "private": true,
  "description": "online music player built by vue",
  "repository": "git@github.com:JounQin/vue-music.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env DEBUG=hi:* node build/bin/start",
    "prod": "cross-env NODE_ENV=production npm run dev",
    "start": "npm run build && npm run prod",
    "build": "rimraf dist && npm run compile",
    "compile:base": "npm run compile:client && npm run compile:server",
    "compile:dev": "cross-env DEBUG=hi:* npm run compile:base",
    "compile": "cross-env NODE_ENV=production npm run compile:dev",
    "compile:client": "webpack --config build/webpack/client.babel.js --progress --hide-modules",
    "compile:server": "webpack --config build/webpack/server.babel.js --progress --hide-modules",
    "lint": "npm run lint:es",
    "lint:fix": "npm run lint:es -- --fix",
    "lint:es": "eslint --ext .js,.vue --max-warnings 10 .",
    "pm2": "cross-env NODE_ENV=production PORT=5000 DEBUG=hi:* pm2 start build/bin/start.js --name=vue-music",
    "deploy": "bash build/bash/deploy.sh"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap-styl": "^5.0.8",
    "encoding": "^0.1.12",
    "fonts.css": "^1.5.0",
    "koa": "^2.3.0",
    "koa-compress": "^2.0.0",
    "koa-logger": "^3.0.1",
    "koa-onerror": "^4.0.0",
    "koa-router": "^7.2.1",
    "koa-static": "^4.0.1",
    "lodash": "^4.17.4",
    "lru-cache": "^4.1.1",
    "mkdirp": "^0.5.1",
    "music-api": "^0.3.9",
    "nib": "^1.1.2",
    "path-to-regexp": "^2.0.0",
    "vue": "^2.4.2",
    "vue-router": "^3.0.0",
    "vue-server-renderer": "^2.4.2",
    "vuex": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.2.11",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "debug": "^3.0.1",
    "eslint": "^4.5.0",
    "eslint-config-1stg": "^1.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.0",
    "img-loader": "^2.0.0",
    "lodash-webpack-plugin": "^0.11.4",
    "null-loader": "^0.1.1",
    "pug": "^2.0.0-rc.3",
    "rimraf": "^2.6.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "stylus-px2rem": "^1.0.14",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^1.0.0",
    "vconsole": "^3.0.0",
    "vue-loader": "^14.0.0",
    "vue-style-loader": "^4.0.0",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^2.0.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-node-externals": "^1.6.0",
    "yargs": "^11.0.0"
  }
}
